stream create --name "myHttpServer1" --definition "queue:myHttpRes > u2ware-http-server-processor --httpPort=9999  --httpTimeout=10000 > queue:myHttpReq"

stream create --name "myHttpServer2" --definition "queue:myHttpReq > u2ware-bacnet-processor --remoteAddress=127.0.0.1:47808 --remoteInstanceNumber=47808 --localPort=47810 --split=false > queue:myHttpRes"

stream deploy --name "myHttpServer2"

stream deploy --name "myHttpServer1"


http post --target http://localhost:9999 --data "{}"

stream destroy --name "myHttpServer1"

stream destroy --name "myHttpServer2"