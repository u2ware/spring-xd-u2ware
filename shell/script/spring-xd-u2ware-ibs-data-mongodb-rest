

stream create --name "testStream" --definition "u2ware-ibs-data-mongodb-rest --httpPort=9898 --host=localhost --port=27017"

stream deploy --name "testStream"

http get --target http://localhost:9898/ 
http get --target http://localhost:9898/person 
http get --target http://localhost:9898/Mina 


stream undeploy --name "testStream"

stream destroy --name "testStream"
